apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: arcadia-ingress
  namespace: my-nginx-ingress
spec:
  ingressClassName: nginx
  rules:
  - host: arcadia1.example.local
    http:
      paths:
      - path: /
        backend:
          serviceName: main
          servicePort: 32765
